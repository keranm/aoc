</div>
<!-- end head message -->
<script type="text/JavaScript">
$(document).ready(function(){
    var categories = 'down';
    var industry = 'up';
    // add for categories
    $('#categories').click(function(){
        if(categories == 'up') {
            $('#cat_toggle').src = '/addons/static/img/Down_24x24.png';
            categories = 'down';
        } else {
            $('#cat_toggle').src = '/addons/static/img/Up_24x24.png';
            categories = 'up';
        }
    })
});
</script>
